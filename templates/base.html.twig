<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
                    <!-- bootstrap css -->
            <link rel="stylesheet" href="brandon/css/bootstrap.min.css">
            <!-- style css -->
            <link rel="stylesheet" href="brandon/css/style.css">
            <link rel="stylesheet" href="css/main.css">
            <!-- Responsive-->
            <link rel="stylesheet" href="brandon/css/responsive.css">
            <!-- fevicon -->
            <link rel="icon" href="brandon/images/fevicon.png" type="image/gif" />
            <!-- Scrollbar Custom CSS -->
            <link rel="stylesheet" href="brandon/css/jquery.mCustomScrollbar.min.css">
            <!-- Tweaks for older IEs-->
            <link rel="stylesheet" href="https://netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css">
            <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/fancybox/2.1.5/jquery.fancybox.min.css" media="screen">

            <!-- CSS del administrador de mesas -->
             <link rel="stylesheet" href="mesas/css/style.css">
             <link rel="stylesheet" href="mesas/ui/css/jquery-ui.css">
        {% endblock %}
 
    </head>
    <body>
        {% block body %}{% endblock %}
        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
            <!-- CSS del administrador de mesas -->
            <script src="mesas/js/jquery-3.6.3.js"></script>
            <script src="mesas/js/jq.js"></script>
            <script src="mesas/js/jquery-ui.js"></script>
            <script src="mesas/mesa.js"></script>

{# 
            <script src="brandon/js/jquery.min.js"></script>
            <script src="brandon/js/popper.min.js"></script>
            <script src="brandon/js/bootstrap.bundle.min.js"></script>
            <script src="brandon/js/jquery-3.0.0.min.js"></script> #}
            <!-- sidebar -->
            <script src="brandon/js/jquery.mCustomScrollbar.concat.min.js"></script>
            <script src="brandon/js/custom.js"></script>
            <script>
                function openNav() {
                document.getElementById("mySidepanel").style.width = "250px";
                }
                
                function closeNav() {
                document.getElementById("mySidepanel").style.width = "0";
                }
        {% endblock %}
    </body>
</html>
